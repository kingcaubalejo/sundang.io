    "dynamodb"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "figlet": "^1.8.2",
    "inquirer": "^12.9.0",
    "ora": "^8.2.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
